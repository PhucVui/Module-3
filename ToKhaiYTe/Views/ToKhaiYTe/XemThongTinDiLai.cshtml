@model ThongTinDiChuyen
@inject ToKhaiYTeContext context

@{
    
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>View</title>
</head>
<body>
    <table id="example" class="table table-striped table-bordered" style="width:100%">
        <thead class="thead-dark" style="text-align:center">
            <tr>
                <th>SoHieuPhuongTien</th>
                <th>Số Ghế</th>
                <th>Trong vòng 21 ngày</th>
                <th>Tàu Bay</th>
                <th>Tàu Thuyền</th>
                <th>Ô Tô</th>
                <th>Khác</th>
            </tr>

        </thead>
        <tbody style="text-align:center">

            <tr>
                <td>@Model.SoHieuPhuongTien</td>
                <td>@Model.SoGhe</td>
                <td>@Model.TrongVong21Ngay</td>
                @if (Model.TauBay)
                {
                    <td>Có</td>
                }
                else
                {
                    <td>Không</td>
                }
                @if (Model.TauThuyen)
                {
                    <td>Có</td>
                }
                else
                {
                    <td>Không</td>
                }
                @if (Model.Oto)
                {
                    <td>Có</td>
                }
                else
                {
                    <td>Không</td>
                }         
                <td>@Model.Khac</td>
            </tr>

        </tbody>
    </table>
    <a type="submit" asp-controller="ToKhaiYTe" asp-action="Index" class="btn btn-danger">Back</a>
  
</body>
</html>
