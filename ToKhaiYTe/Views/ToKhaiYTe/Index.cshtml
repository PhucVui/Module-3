@model List<NguoiKhaiBao>
@inject ToKhaiYTeContext context;

@{
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>View</title>
</head>
<body>
    <table id="example" class="table table-striped table-bordered" style="width:100%">
        <thead class="thead-dark" style="text-align:center">
            <tr>
                <th>Họ Tên</th>
                <th>Số Hộ Chiếu</th>
                <th>Cửa Khẩu</th>
                <th>Thông tin di chuyển</th>
                <th>Thông tin liên lạc</th>
                <th>Triệu chứng</th>
                <th>Phơi nhiễm</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody style="text-align:center">
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.HoTen</td>
                    <td>@item.SoHoChieu</td>
                    <td>@context.Gates.FirstOrDefault(el => el.Id == item.GateId).GateName</td>

                    <td>
                        <a asp-controller="ToKhaiYTe" asp-action="XemThongTinDiLai"
                           asp-route-id="@item.ThongTinDiChuyenId">
                            Xem chi tiết
                        </a>
                    </td>

                    <td>
                        <a asp-controller="ToKhaiYTe" asp-action="XemThongTinLienLac"
                           asp-route-id="@item.ThongTinLienLacId">
                            Xem chi tiết
                        </a>
                    </td>
                    <td>
                        <a asp-controller="ToKhaiYTe" asp-action="XemDauHieu"
                           asp-route-id="@item.DauHieuId">
                            Xem chi tiết
                        </a>
                    </td>
                    <td>
                        <a asp-controller="ToKhaiYTe" asp-action="XemPhoiNhiem"
                           asp-route-id="@item.PhoiNhiemId">
                            Xem chi tiết
                        </a>
                    </td>
                    <td>
                        <a href="javascript:void(0)" class="text-danger" onclick="ToKhai.delete(@item.Id)">Delete</a>
                    </td>
                </tr>
            }
        </tbody>

    </table>
    <a type="submit" asp-controller="ToKhaiYTe" asp-action="Register" class="btn btn-danger">Back</a>
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/lib/bootbox/bootbox.js"></script>
</body>
</html>
    
@section Scripts{
    <script src="~/js/Create.js" asp-append-version="true"></script>
}
